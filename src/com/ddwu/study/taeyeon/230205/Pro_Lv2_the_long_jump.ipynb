   "source": [
    "def solution(n):\n",
    "    dp = [1] + [0] * n\n",
    "    dp[0], dp[1] = 1, 1\n",
    "    for i in range(2, n+1):\n",
    "        dp[i] = (dp[i-1] + dp[i-2]) % 1234567\n",
    "        \n",
    "    return dp[n]"
   ]
