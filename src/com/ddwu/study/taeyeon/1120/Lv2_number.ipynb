   "source": [
    "cnt = 1\n",
    "\n",
    "for i in range(1, n):\n",
    "    sum = i\n",
    "    for j in range(i+1, n):\n",
    "        sum += j\n",
    "        if sum == n:\n",
    "            cnt += 1\n",
    "            break\n",
    "        elif sum > n:\n",
    "            break\n",
    "\n",
    "print(cnt)"
   ]
