   "source": [
    "def solution(n):\n",
    "    dp = [0 for i in range(n)]\n",
    "    dp[0], dp[1] = 1, 2\n",
    "    for i in range(2, n):\n",
    "        dp[i] = (dp[i-1] + dp[i-2]) % 1000000007\n",
    "\n",
    "    return dp[n-1]"
   ]
