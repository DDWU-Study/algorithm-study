   "source": [
    "def solution(n):\n",
    "    answer = ''\n",
    "    \n",
    "    while(n >= 1):\n",
    "        rest = n % 3 # 나머지\n",
    "        n = n // 3 # 몫\n",
    "        answer += str(rest) #나머지를 순서대로 저장 (순서대로 저장 = 앞뒤 반전)\n",
    "    \n",
    "    return int(answer, 3) # int()로 10진법 변경"
   ]
  },
